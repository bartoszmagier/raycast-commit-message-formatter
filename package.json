{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "commit-message-formatter",
  "title": "Commit Message Formatter",
  "description": "Parse GIT Branch Name into a Commit Message",
  "icon": "commit-message-formatter.png",
  "author": "bartoszmagier",
  "version": "0.0.1",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Format Commit Message",
      "description": "Parse GIT Branch Name into a Commit Message",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}